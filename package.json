{
  "name": "go-arrival",
  "version": "0.0.0",
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "no-console": "error",
      "no-undef": "error",
      "no-unused-vars": "warn",
      "no-extra-semi": "warn"
    }
